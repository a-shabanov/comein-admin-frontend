{
  "name": "@nexcella/comein-web",
  "version": "1.5.1",
  "description": "Web application for ComeIn admin panel",
  "contributors": [
    {
      "name": "Andrey Shabanov",
      "email": "a.shabanov.develop@gmail.com"
    }
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "test": "tsc -p .",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nexcella/comein-api": "0.0.13",
    "@nexcella/logger": "1.0.0",
    "@types/yup": "0.29.11",
    "axios": "0.21.1",
    "formik": "2.2.6",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "localforage": "1.9.0",
    "mobx": "6.1.4",
    "mobx-react": "7.1.0",
    "mobx-sync": "3.0.0",
    "nanoid": "3.1.20",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-google-login": "5.2.2",
    "react-i18next": "11.8.5",
    "react-router-dom": "5.2.0",
    "typescript": "4.1.3",
    "webpack": "4.46.0",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/axios": "0.14.0",
    "@types/nanoid": "2.1.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack": "4.41.26",
    "@types/webpack-dev-server": "3.11.1",
    "astroturf": "0.10.5",
    "conventional-changelog-eslint": "3.0.9",
    "dotenv": "8.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.1",
    "mini-css-extract-plugin": "1.3.4",
    "node-sass": "5.0.0",
    "sass-loader": "10.1.1",
    "semantic-release": "17.3.3",
    "semantic-release-cli": "5.4.1",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-loader": "8.0.14",
    "url-loader": "4.1.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexcella/comein-admin-frontend.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      {
        "name": "release/([0-9]+)\\.([0-9]+)\\.([0-9]+)",
        "channel": "rc",
        "prerelease": "rc"
      },
      {
        "name": "develop",
        "prerelease": "beta"
      },
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint",
          "releaseRules": [
            {
              "subject": "PATCH-RELEASE:*",
              "release": "patch"
            },
            {
              "subject": "MINOR-RELEASE:*",
              "release": "minor"
            },
            {
              "subject": "MAJOR-RELEASE:*",
              "release": "major"
            },
            {
              "subject": "*",
              "release": false
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ]
  }
}
